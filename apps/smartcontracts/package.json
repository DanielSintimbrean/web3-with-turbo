{
  "name": "@turbo-web3/smartcontracts",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "type": "commonjs",
  "private": true,
  "exports": {
    "./typechain-types": {
      "import": "./dist/typechain-types/index.mjs",
      "require": "./dist/typechain-types/index.js"
    },
    "./network-mapping": {
      "import": "./dist/network-mapping/index.mjs",
      "require": "./dist/network-mapping/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "typechain-types": [
        "./dist/typechain-types/index.d.ts"
      ],
      "network-mapping": [
        "./dist/network-mapping/index.d.ts"
      ]
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf .next .turbo node_modules && pnpm hardhat clean",
    "dev": "hardhat run ./scripts/dev-network.ts",
    "lint": "eslint && tsc",
    "build": "tsup",
    "compile": "hardhat compile",
    "type:generate": "hardhat typechain",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "esbuild": "^0.17.11",
    "hardhat": "^2.13.0",
    "npm-dts": "^1.3.12",
    "ts-toolbelt": "^9.6.0",
    "tsup": "^6.6.3"
  }
}
